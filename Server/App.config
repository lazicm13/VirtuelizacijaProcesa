<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<services>
			<service name="Server.ServiceLoad">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4000"/>
					</baseAddresses>
				</host>
				<endpoint address="ILoad" binding="netTcpBinding" contract="Common.ILoad" />
			</service>
		</services>
	</system.serviceModel>
	<appSettings>
		<add key="OdstupanjeMetoda" value="KvOdstupanje" />
		<add key="VrstaUpisa" value="XMLDatabase" />
	</appSettings>
</configuration>